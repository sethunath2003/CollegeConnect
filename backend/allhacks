</section>    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="/newui/js/home.js"></script>
    <script>
        // Function to filter hackathons by name or description
        const filterHackathons = () => {
            const input = document.getElementById("hackathonFilter");
            const filter = input.value.toUpperCase();
            const hackathonList = document.getElementById("hackathonList");
            const hackathonCards = hackathonList.getElementsByClassName("hackathonCard");

            for (let i = 0; i < hackathonCards.length; i++) {
                const name = hackathonCards[i].querySelector(".eventName").innerText.toUpperCase();
                const description = hackathonCards[i].querySelector(".eventDescription").innerText.toUpperCase();

                if (name.indexOf(filter) > -1 || description.indexOf(filter) > -1) {
                    hackathonCards[i].style.display = "";
                } else {
                    hackathonCards[i].style.display = "none";
                }
            }
        };
    </script>
